<%= form_with model: @user, url: admin_user_path(@user), method: :patch, local: true do |f| %>
    <div class="mb-3">
      <p>ユーザー名</p>
        <%= f.text_field :name %>
    </div>
    <div class="mb-3">
      <p>メールアドレス</p>
        <%= f.text_field :email %>
    </div>
    <%= f.submit '登録' , class: 'btn btn-primary'%>
  </div>
<% end %>